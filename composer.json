{
    "name": "stubbles/reflect",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Reflection helper functions and annotations.",
    "require": {
        "php": "^8.0",
        "stubbles/sequence": "^10.0",
        "stubbles/values": "^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mikey179/vfsstream": "^1.6.11",
        "bovigo/assert": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "stubbles\\reflect\\": "src/main/php"
        },
        "files": ["src/main/php/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "stubbles\\reflect\\": "src/test/php"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "10.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit",
        "php-coveralls": "php-coveralls"
    }
}

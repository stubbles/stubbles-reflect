{
    "name": "stubbles/reflect",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Reflection helper functions and annotations.",
    "require": {
        "php": "^8.3",
        "stubbles/sequence": "^11.0",
        "stubbles/values": "^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.4",
        "mikey179/vfsstream": "^1.6.12",
        "bovigo/assert": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "stubbles\\reflect\\": "src/main/php"
        },
        "files": ["src/main/php/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "stubbles\\reflect\\": "src/test/php"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "11.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit"
    }
}
